@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="m-subheader">
    <div class="d-flex align-items-center">
        <div class="mr-auto">
            <h2 class="m-subheader__title m-subheader__title--separator">Non Standar&nbsp;&nbsp;</h2>
            <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
                <li class="m-nav__item m-nav__item--home">
                    <a href="javascript:return;" class="m-nav__link m-nav__link--icon">
                        <i class="fa fa fa-calendar"></i>
                    </a>
                </li>
                <li class="m-nav__item m-nav__separator"><i class="fa fa-angle-double-right"></i></li>
                <li class="m-nav__item">
                    <a href="javascript:return;" class="m-nav__link">
                        <span class="m-nav__link-text">List Of Non Standar</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>
<div class="m-content" style="padding-left:20px; padding-right:20px">
    <div class="row">
        <div class="col-xs-12 box-content">
            <div class="portlet-body">
                <div class="box-body no-padding">
                    <form class="form-horizontal" role="form" id="form" id="arpayment-index">
                        <div class="form-body">
                            <div id="datatable_fixed_column_wrapper" class="dataTables_wrapper form-inline no-footer">
                                <div class="dt-toolbar">
                                </div>
                                <table id="non_standar_table_header" class="datatable table table-striped    table-responsive table-bordered dataTable no-footer" role="grid" style="font-size:12px">
                                    <thead style="font-size:10px">
                                        <tr role="row">
                                            <th class="" tabindex="0" width="3%">NO</th>
                                            <th class="" tabindex="0" aria-controls="non_standar_table_header" width="15%" id="ksmNumber" aria-sort="ascending">KSM Number</th>
                                            <th tabindex="0" aria-controls="non_standar_table_header" id="locationMap" aria-sort="ascending">Location</th>
                                            <th tabindex="0" aria-controls="non_standar_table_header" id="square" width="15%" aria-sort="ascending">Square</th>
                                            <th tabindex="0" aria-controls="non_standar_table_header" id="outletName" width="15%" aria-sort="ascending">Outlet Name</th>
                                            <th tabindex="0" aria-controls="non_standar_table_header" id="tenantOwner" width="15%" aria-sort="ascending">Unit Name</th>
                                            <th tabindex="0" aria-controls="non_standar_table_header" id="chargeDateFrom" aria-sort="ascending" data-format-string="{0:N}">Charge Date From</th>
                                            <th tabindex="0" aria-controls="non_standar_table_header" id="chargeDateTo" aria-sort="ascending" data-format-string="{0:N}">Charge Date To</th>
                                            <th tabindex="0" aria-controls="non_standar_table_header">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <div class="dt-toolbar-footer">
                                    <div class="col-sm-6 col-xs-12 hidden-xs"></div>
                                    <div class="col-xs-12 col-sm-6"></div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <script type="text/javascript" src="~/js/jsfile/functions.js?v1.1"></script>
    <script type="text/javascript" src="~/js/jsfile/NonStandard/Index.js?v1.5"></script>
} 