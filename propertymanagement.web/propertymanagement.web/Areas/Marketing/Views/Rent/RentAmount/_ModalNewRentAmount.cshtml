<div class="modal fade" id="modalNewRentAmount" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <!-- Modal Header -->
            <div class="modal-header">
                <button type="button" class="close" id="iconClose" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">New Rent Amount</h4>
                <br />
            </div>
            <div id="gridDetail" class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group row">
                            <label class="col-md-5 control-label">Month From</label>
                            <div class="col-md-7">
                                <input type="number" id="monthFromModal" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-md-5 control-label">Month To</label>
                            <div class="col-md-7">
                                <input type="number" id="monthToModal" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-md-5 control-label">Price (Per Month Per m2)</label>
                            <div class="col-md-7">
                                <input type="text" id="priceModal" class="form-control" />
                            </div>
                        </div>
                        <div class="form-group text-right">
                            <button class="btn btn-primary" id="save_item_rent_amount">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <script src="~/js/jsfile/RentAmount/AddEdit.js"></script>
    <script type="text/javascript" src="~/js/jsfile/functions.js?v1.0"></script>
}
