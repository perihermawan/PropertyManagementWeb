<div class="modal fade" id="modalUnit" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog modal-md" style="width:1350px;">
        <div class="modal-content">
            <!-- Modal Header -->
            <div class="modal-header">
                <button type="button" class="close" id="iconClose" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Unit List</h4>
                <br />
            </div>
            <div id="gridDetail" class="">
                <div class="table-responsive mr-lg-5">
                    <table id="unit_table_header" class="datatable table table-striped table-responsive table-bordered dataTable no-footer" role="grid" style="font-size:12px">
                        <thead style="font-size:10px">
                            <tr role="row">
                                <th class="" width="5%">NO</th>
                                <th id="mapNumber">Map Number</th>
                                <th id="rentSquare">Square</th>
                                <th id="power">Power</th>
                                <th header="status" id="statusDesc">Status</th>
                                <th id="titipSewaFlag">Titip Sewa</th>
                                <th id="ownerName">Owner Name</th>
                                <th width="10%"><input type="checkbox" id="checkUnitAll" /></th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>

            </div>
            <div class="model-footer" style="margin:20px!important; text-align:end!important">
                <button type="button" class="btn btn-primary" id="btnSelectUnit">Add</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

