@{
    var PipeType = ViewBag.PipeType;
    var PressureType = ViewBag.PressureType;
}
<div class="modal fade" id="modalEditNewUnit" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog modal-md" style="width:900px;">
        <div class="modal-content">
            <!-- Modal Header -->
            <div class="modal-header">
                <button type="button" class="close" id="iconClose" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Edit New Unit List</h4>
                <br />
            </div>
            <div class="modal-body">
                <h5><b>Rent Square (m<sup>2</sup>)</b></h5>
                <div class="form-group row">
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">Orig</label>
                        <div class="col-md-8">
                            <input type="text" name="rentSquareOrig" id="rentSquareOrig" class="form-control" style="padding-left:0px; padding-right:0px" />
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">Fac</label>
                        <div class="col-md-8">
                            <input type="text" name="rentSquareFac" id="rentSquareFac" class="form-control" style="padding-left:0px; padding-right:0px" />
                        </div>
                    </div>
                    <div class="col-md-4">
                    </div>
                </div>

                <h5><b>Rent PLN</b></h5>
                <div class="form-group row">
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">PLN Serial Number</label>
                        <div class="col-md-8">
                            <input type="text" name="rentPlnSerialNo2" id="rentPlnSerialNo2" class="form-control" style="padding-left:0px; padding-right:0px" />
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">CT Factor</label>
                        <div class="col-md-8">
                            <input type="text" name="rentPlnCtFactor" id="rentPlnCtFactor" class="form-control" style="padding-left:0px; padding-right:0px" />
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">Power</label>
                        <div class="col-md-8">
                            <input type="text" name="rentPlnPower" id="rentPlnPower" class="form-control" style="padding-left:0px; padding-right:0px" />
                        </div>
                    </div>
                </div>

                <h5><b>Rent PAM</b></h5>
                <div class="form-group row">
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">Serial No.</label>
                        <div class="col-md-8">
                            <input type="text" name="rentPamSerialNo" id="rentPamSerialNo" class="form-control" style="padding-left:0px; padding-right:0px" />
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">Pipe Inch</label>
                        <div class="col-md-8">
                            <select class="form-control" name="rentPamPipeInch" id="rentPamPipeInch" style="display: inline;">
                                <option>-- Please Select --</option>
                                @{
                                    if (PipeType != null)
                                    {
                                        foreach (var item in PipeType)
                                        {
                                            <option value="@item.pipeTypeId">@item.pipeTypeName</option>
                                        }

                                    }
                                }
                            </select>
                        </div>
                    </div>
                    <div class="col-md-4">
                    </div>
                </div>

                <h5><b>Rent GAS</b></h5>
                <div class="form-group row">
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">Serial No.</label>
                        <div class="col-md-8">
                            <input type="text" name="rentGasSerialNo" id="rentGasSerialNo" class="form-control" style="padding-left:0px; padding-right:0px" />
                        </div>
                    </div>
                    <div class="col-md-4">
                        <label class="col-md-4 control-label">Pressure</label>
                        <div class="col-md-8">
                            <select class="form-control" name="rentGasPressure" id="rentGasPressure" style="display: inline;">
                                <option>-- Please Select --</option>
                                @{
                                    if (PressureType != null)
                                    {
                                        foreach (var item in PressureType)
                                        {
                                            <option value="@item.pressureTypeId">@item.pressureTypeName</option>
                                        }

                                    }
                                }
                            </select>
                        </div>
                    </div>
                    <div class="col-md-4">
                    </div>
                </div>
            </div>
            <div class="model-footer" style="margin:20px!important; text-align:end!important">
                <button type="button" class="btn btn-primary" id="saveEditNewUnit" onclick="saveEditNewUnit()">Save</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

@section Scripts{
}
