<div class="modal fade" id="modalAddEdit" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog modal-md" style="width:950px;">
        <div class="modal-content">
            <!-- Modal Header -->
            <div class="modal-header">
                <button type="button" class="close" id="iconClose" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">PSM</h4>
                <br />
            </div>
            <div id="gridDetail" class="">
                <div class="table-responsive mr-lg-5">
                    <table id="psm_table_header" class="datatable table table-striped table-responsive table-bordered dataTable no-footer" role="grid" style="font-size:12px">
                        <thead style="font-size:10px">
                            <tr role="row">
                                <th class="" tabindex="0" width="10%">No</th>
                                <th tabindex="0" aria-controls="psm_table_header" id="psmNumber" aria-sort="ascending">PSM Number</th>
                                <th tabindex="0" aria-controls="psm_table_header" id="locationMap" aria-sort="ascending">Location</th>
                                <th tabindex="0" aria-controls="psm_table_header" id="square" aria-sort="ascending">Square</th>
                                <th tabindex="0" aria-controls="psm_table_header" id="outletName" aria-sort="ascending">Outlet Name</th>
                                <th tabindex="0" aria-controls="psm_table_header" id="unitOwner" aria-sort="ascending">Unit Owner</th>
                                <th tabindex="0" aria-controls="psm_table_header" id="tenantOwner" aria-sort="ascending">Tenant Owner</th>
                                <th tabindex="0" aria-controls="psm_table_header">Action</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>

            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->