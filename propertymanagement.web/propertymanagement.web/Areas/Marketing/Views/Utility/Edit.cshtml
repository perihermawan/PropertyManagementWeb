                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-6">
                            <label class="radio-inline">
                                <input type="radio" name="mfType" id="mfTypeRent" @(code == "Rent" ? "checked" : "disabled")> Rent
                            </label>
                            <label class="radio-inline">
                                <input type="radio" name="mfType" id="mfTypeSales" @(code == "Sales" ? "checked" : "disabled")> Sales
                            </label>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="sourceId" for="sourceIdDisp">PSM Number</label>
                                <div class="input-group customValidate">
                                    <input type="hidden" name="sourceId" value="@(code == "Rent" ? tenantMgt.tenantRent[0].SourceId : code == "Sales" ? tenantMgt.tenantSales[0].SourceId : "")" />
                                    <input type="text" class="form-control isrequired-field" name="sourceIdDisp" id="sourceIdDisp" value="@(code == "Rent" ? tenantMgt.tenantRent[0].PSMNumber : code == "Sales" ? tenantMgt.tenantSales[0].OrderNumber : "")" disabled />
                                    <div class="input-group-btn">
                                        <button class="btn btn-default" type="button" style="padding-top: 9px;padding-bottom: 9px;"><span class="glyphicon glyphicon-search"></span></button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="outletType">Outlet Type</label>
                                <input type="text" class="form-control" name="outletType" id="outletType" value="@(code == "Rent" ? tenantMgt.tenantRent[0].OutletType : code == "Sales" ? tenantMgt.tenantSales[0].OutletType : "")" disabled />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="location">Location</label>
                                <input type="text" class="form-control" name="location" id="location" value="@(code == "Rent" ? tenantMgt.tenantRent[0].LocationMap : code == "Sales" ? tenantMgt.tenantSales[0].LocationMap : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="checkbox-inline" style="padding-left:0px">
                                    Food Court
                                    <input type="checkbox" name="foodCourt" id="foodCourt" value="1" style="margin-left:10px;" @(tenantMgt.tenantRent != null ? tenantMgt.tenantRent[0].IsFoodCourt ? "checked" : tenantMgt.tenantSales != null ? tenantMgt.tenantSales[0].IsFoodCourt ? "checked" : "" : "" : "") disabled>
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="square">Square</label>
                                <input type="text" class="form-control" name="square" id="square" value="@(tenantMgt != null ? (code == "Rent" ? tenantMgt.tenantRent[0].Square : code == "Sales" ? tenantMgt.tenantSales[0].Square : "") : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="checkbox-inline" style="padding-left:0px">
                                    Display Area
                                    <input type="checkbox" name="displayArea" id="displayArea" value="1" style="margin-left:10px;" @(code == "Rent" ? tenantMgt.tenantRent[0].IsDisplayArea ? "checked" : code == "Sales" ? tenantMgt.tenantSales[0].IsDisplayArea ? "checked" : "" : "" : "") disabled />
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="outletName">Outlet Name</label>
                                <input type="text" class="form-control" name="outletName" id="outletName" value="@(code == "Rent" ? tenantMgt.tenantRent[0].OutletName : code == "Sales" ? tenantMgt.tenantSales[0].OutletName : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="handOverDate">Hand Over Date Real</label>
                                <input type="text" class="form-control" name="handOverDate" id="handOverDate" value="@(code == "Rent" ? tenantMgt.tenantRent[0].HandOverDate : code == "Sales" ? tenantMgt.tenantSales[0].HandOverDate : "")" disabled />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="unitOwner">Unit Owner</label>
                                <input type="text" class="form-control" name="unitOwner" id="unitOwner" value="@(code == "Rent" ? tenantMgt.tenantRent[0].UnitOwner : code == "Sales" ? tenantMgt.tenantSales[0].UnitOwner : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="openDate">Open Date (Real)</label>
                                <input type="text" class="form-control" name="openDate" id="openDate" value="@(code == "Rent" ? tenantMgt.tenantRent[0].OpenDate : "")" disabled />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="tenantOwner">Tenant Owner</label>
                                <input type="text" class="form-control" name="tenantOwner" id="tenantOwner" value="@(code == "Rent" ? tenantMgt.tenantRent[0].TenantOwner : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="rentStartDate">Rent Start Date</label>
                                <input type="text" class="form-control" name="rentStartDate" id="rentStartDate" value="@(code == "Rent" ? tenantMgt.tenantRent[0].StartDate : "")" disabled />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-4">
                            <input type="hidden" name="tenantMgtId" id="tenantMgtIdE" data-skip-validate="@(dataERent.TenantMgtId != null ? true : dataESales.TenantMgtId != null ? true : false)" value="@(dataERent.TenantMgtId != null ? dataERent.TenantMgtId : dataESales.TenantMgtId != null ? dataESales.TenantMgtId : "")" />
                            <input type="hidden" name="prmTenantMgtId" id="prmTenantMgtIdE" data-skip-validate="true" value="@(dataERent.TenantMgtId != null ? dataERent.PrmTenantMgtId : dataESales.TenantMgtId != null ? dataESales.PrmTenantMgtId : "")" />
                            <label class="checkbox-inline customValidate">
                                <input class="isrequired-field" type="checkbox" name="pln" id="pln" data-isGroup="type" value="E" @(action == "view" ? "" : "onclick=" + "selectE(this);" + "") @(action == "view" ? "readOnly" : "") @(dataERent.TenantMgtId != null ? "checked" : dataESales.TenantMgtId != null ? "checked" : "") />
                                Electric
                            </label>
                        </div>
                        <div class="col-md-4">
                            <input type="hidden" name="tenantMgtId" id="tenantMgtIdW" data-skip-validate="@(dataWRent.TenantMgtId != null ? true : dataWSales.TenantMgtId != null ? true : false)" value="@(dataWRent.TenantMgtId != null ? dataWRent.TenantMgtId : dataWSales.TenantMgtId != null ? dataWSales.TenantMgtId : "")" />
                            <input type="hidden" name="prmTenantMgtId" id="prmTenantMgtIdW" data-skip-validate="true" value="@(dataWRent.TenantMgtId != null ? dataWRent.PrmTenantMgtId : dataWSales.TenantMgtId != null ? dataWSales.PrmTenantMgtId : "")" />
                            <label class="checkbox-inline customValidate">
                                <input class="isrequired-field" type="checkbox" name="pam" id="pam" data-isGroup="type" value="W" @(action == "view" ? "" : "onclick=" + "selectW(this);" + "") @(action == "view" ? "readOnly" : "") @(dataWRent.TenantMgtId != null ? "checked" : dataWSales.TenantMgtId != null ? "checked" : "") />
                                Water
                            </label>
                        </div>
                        <div class="col-md-4">
                            <input type="hidden" name="tenantMgtId" id="tenantMgtIdG" data-skip-validate="@(dataGRent.TenantMgtId != null ? true : dataGSales.TenantMgtId != null ? true : false)" value="@(dataGRent.TenantMgtId != null ? dataGRent.TenantMgtId : dataGSales.TenantMgtId != null ? dataGSales.TenantMgtId : "")" />
                            <input type="hidden" name="prmTenantMgtId" id="prmTenantMgtIdG" data-skip-validate="true" value="@(dataGRent.TenantMgtId != null ? dataGRent.PrmTenantMgtId : dataGSales.TenantMgtId != null ? dataGSales.PrmTenantMgtId : "")" />
                            <label class="checkbox-inline customValidate">
                                <input class="isrequired-field" type="checkbox" name="gas" id="gas" data-isGroup="type" value="G" @(action == "view" ? "" : "onclick=" + "selectG(this);" + "") @(action == "view" ? "readOnly" : "") @(dataGRent.TenantMgtId != null ? "checked" : dataGSales.TenantMgtId != null ? "checked" : "") />
                                Gas
                            </label>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="chargeDateFrom">Consumption From</label>
                                <input class="form-control" name="chargeDateFrom" id="serviceFromE" data-provide="datepicker" value="@(dataERent != null ? dataERent.ChargeDateFrom : dataESales != null ? dataESales.ChargeDateFrom : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <input class="form-control" name="chargeDateFrom" id="serviceFromW" data-provide="datepicker" value="@(dataWRent != null ? dataWRent.ChargeDateFrom : dataWSales != null ? dataWSales.ChargeDateFrom : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <input class="form-control" name="chargeDateFrom" id="serviceFromG" data-provide="datepicker" value="@(dataGRent != null ? dataGRent.ChargeDateFrom : dataGSales != null ? dataGSales.ChargeDateFrom : "")" disabled />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="chargeEvery">Charge Every</label>
                                <select class="form-control" name="chargeEvery" id="chargeEveryE" value="" disabled></select>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <select class="form-control" name="chargeEvery" id="chargeEveryW" value="" disabled></select>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <select class="form-control" name="chargeEvery" id="chargeEveryG" value="" disabled></select>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="chargeToId">Charge To</label>
                                <select class="form-control" name="chargeToId" id="chargeToE" value="" disabled></select>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <select class="form-control" name="chargeToId" id="chargeToW" value="" disabled></select>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <select class="form-control" name="chargeToId" id="chargeToG" value="" disabled></select>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Lump-Sump</label>
                                <label class="form-control checkbox-inline" style="border: none; pointer-events: none;">
                                    <input type="checkbox" name="isLumpSump" id="isLumpSumpE" data-skip-validate="true" value="@(dataERent != null ? Convert.ToInt32(dataERent.IsLumpSump) : dataESales != null ? Convert.ToInt32(dataESales.IsLumpSump) : "")" onclick="selectLumpSump(this)" />
                                    No
                                </label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <label class="form-control checkbox-inline" style="border: none; pointer-events: none;">
                                    <input type="checkbox" name="isLumpSump" id="isLumpSumpW" data-skip-validate="true" value="@(dataWRent != null ? Convert.ToInt32(dataWRent.IsLumpSump) : dataWSales != null ? Convert.ToInt32(dataWSales.IsLumpSump) : "")" onclick="selectLumpSump(this)" />
                                    No
                                </label>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <label class="form-control checkbox-inline" style="border: none; pointer-events: none;">
                                    <input type="checkbox" name="isLumpSump" id="isLumpSumpG" data-skip-validate="true" value="@(dataGRent != null ? Convert.ToInt32(dataGRent.IsLumpSump) : dataGSales != null ? Convert.ToInt32(dataGSales.IsLumpSump) : "")" onclick="selectLumpSump(this)" />
                                    No
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="chargeAmountOther">Charge Amount</label>
                                <input type="text" class="form-control" name="chargeAmountOther" id="chargeAmountOtherE" value="@(dataERent != null ? (dataERent.ChargeAmountOther == 0 ? 0 : dataERent.ChargeAmountOther) : dataESales != null ? (dataESales.ChargeAmountOther == 0 ? 0 : dataESales.ChargeAmountOther) : "")" onkeypress="return isNumberKey(event)" disabled />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <input type="text" class="form-control" name="chargeAmountOther" id="chargeAmountOtherW" value="@(dataWRent != null ? (dataWRent.ChargeAmountOther == 0 ? 0 : dataWRent.ChargeAmountOther) : dataWSales != null ? (dataWSales.ChargeAmountOther == 0 ? 0 : dataWSales.ChargeAmountOther) : "")" onkeypress="return isNumberKey(event)" disabled />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <input type="text" class="form-control" name="chargeAmountOther" id="chargeAmountOtherG" value="@(dataGRent != null ? (dataGRent.ChargeAmountOther == 0 ? 0 : dataGRent.ChargeAmountOther) : dataGSales != null ? (dataGSales.ChargeAmountOther == 0 ? 0 : dataGSales.ChargeAmountOther) : "")" onkeypress="return isNumberKey(event)" disabled />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 col-no-padding" style="margin-bottom: 15px;">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label for="node">Node</label>
                                <input type="text" class="form-control" name="node" id="nodeE" data-skip-validate="true" value="@(dataERent != null ? dataERent.PlnNode : dataESales != null ? dataESales.PlnNode : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <input type="text" class="form-control" name="node" id="nodeW" data-skip-validate="true" value="@(dataWRent != null ? dataWRent.PamNode : dataWSales != null ? dataWSales.PamNode : "")" disabled />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label style="margin-bottom: 25px;"></label>
                                <input type="text" class="form-control" name="node" id="nodeG" data-skip-validate="true" value="@(dataGRent != null ? dataGRent.GasNode : dataGSales != null ? dataGSales.GasNode : "")" disabled />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12" style="padding: 15px;margin-top: 15px;margin-bottom: 15px;font-size: 14px;font-weight: 500;background-color: #485b8d;color: #fff;">Utility Parameter</div>
                    <div class="col-xs-12 col-md-12" style="margin-bottom: 15px;">
                        @{
                            var totPower = 0;
                            var isParam = false;
                            if (dataERent != null || dataESales != null)
                            {
                                totPower = dataERent != null ? dataERent.TotPower : dataESales != null ? dataESales.TotPower : 0;
                                isParam = true;
                            }
                            else if (dataWRent != null || dataWSales != null)
                            {
                                totPower = dataWRent != null ? dataWRent.TotPower : dataWSales != null ? dataWSales.TotPower : 0;
                                isParam = true;
                            }
                            else if (dataGRent != null || dataGSales != null)
                            {
                                totPower = dataGRent != null ? dataGRent.TotPower : dataGSales != null ? dataGSales.TotPower : 0;
                                isParam = true;
                            }
                         }
                        <div class="col-md-4 form-inline totPower" style="@(totPower > 0 ? "" : "display:none;")">
                            <div class="form-group">
                                <label for="totPower">Power Electric</label>
                                <input type="text" class="form-control" name="totPower" id="totPower" value="@(totPower > 0 ? totPower : "")" disabled />
                            </div>
                        </div>
                        <table id="utility_param_table_header" class="datatable table table-striped table-responsive table-bordered" style="@(isParam ? "" : "display: none;")">
                            <thead style="font-size:10px">
                                <tr role="row">
                                    <th tabindex="0">NO</th>
                                    <th tabindex="1" aria-controls="utility_param_table_header" id="name" headers="id" aria-sort="ascending">Name</th>
                                    <th tabindex="2" aria-controls="utility_param_table_header" id="subscriptionFee" aria-sort="ascending">Subscription Fee</th>
                                    <th tabindex="3" aria-controls="utility_param_table_header" id="maintenanceFee" aria-sort="ascending">Maintenance Fee</th>
                                    <th tabindex="4" aria-controls="utility_param_table_header" id="usageFee1" aria-sort="ascending">Usage Fee < 125 KVA</th>
                                    <th tabindex="5" aria-controls="utility_param_table_header" id="usageFee2" aria-sort="ascending">Usage Fee >= 125 KVA</th>
                                    <th tabindex="6" aria-controls="utility_param_table_header" id="PpjPercentage" aria-sort="ascending">% Ppj</th>
                                    <th tabindex="7" aria-controls="utility_param_table_header" id="AdmPercentage" aria-sort="ascending">% Adm</th>
                                    <th tabindex="7" aria-controls="utility_param_table_header" id="DiscPercentage" aria-sort="ascending">% Disc</th>
                                    <th tabindex="7" aria-controls="utility_param_table_header" id="TaxPercentage" aria-sort="ascending">% Vat</th>
                                    <th tabindex="8" aria-controls="utility_param_table_header" id="MINIMUMAMOUNT" aria-sort="ascending">Minimum Amount</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    if (tenantMgt.tenantRent != null)
                                    {
                                        var rowNum = 0;
                                        foreach (var item in tenantMgt.tenantRent)
                                        {
                                            rowNum++;
                                            <tr>
                                                <td>@(rowNum)</td>
                                                <td>@(item.Name)</td>
                                                <td>@(item.SubscriptionFee)</td>
