@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using propertymanagement.web.Areas.Marketing.Models.ViewModel;
@model propertymanagement.web.Areas.Marketing.Models.DTO.ViewTenantMgtRentSales;
@{
    var action = ViewBag.Action;
    var code = ViewBag.Code;
    var tenantMgt = Model != null ? Model : null;

    var dataERent = new ViewTenantMgtRentModel();
    var dataESales = new ViewTenantMgtSalesModel();
    var dataWRent = new ViewTenantMgtRentModel();
    var dataWSales = new ViewTenantMgtSalesModel();
    var dataGRent = new ViewTenantMgtRentModel();
    var dataGSales = new ViewTenantMgtSalesModel();
    if (tenantMgt != null)
    {
        if (code == "Rent")
        {
            if (tenantMgt.tenantRent != null)
            {
                foreach (var item in tenantMgt.tenantRent)
                {
                    if (item.Code == "E")
                    {
                        dataERent = item;
                    }
                    else if (item.Code == "W")
                    {
                        dataWRent = item;
                    }
                    else if (item.Code == "G")
                    {
                        dataGRent = item;
                    }
                }
            }
        }
        else if (code == "Sales")
        {
            if (tenantMgt.tenantSales != null)
            {
                foreach (var item in tenantMgt.tenantSales)
                {
                    if (item.Code == "E")
                    {
                        dataESales = item;
                    }
                    else if (item.Code == "W")
                    {
                        dataWSales = item;
                    }
                    else if (item.Code == "G")
                    {
                        dataGSales = item;
                    }
                }
            }
        }
    }
}

<style type="text/css">
    .req:after {
        content: " *";
        color: red;
    }

    .col-no-padding {
        padding: 0px;
    }

    .line-separator-1 {
        width: 100%;
        margin: 5px 0px 15px;
        border: 1px solid #bababa3d;
    }

    #form-mf input, #form-mf select {
        max-width: unset;
    }

    @@media screen and (min-width: 992px) {
        .modal-lg {
            width: 988px;
        }
    }

    .btn {
        border-radius: 4px !important;
    }
</style>

<div class="m-subheader">
    <div class="d-flex align-items-center">
        <div class="mr-auto">
            <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
                <li class="m-nav__item">
                    <a asp-action="Index" asp-controller="Unit" class="m-nav__link">
                        <span class="m-nav__link-text">Utility</span>
                    </a>
                </li>
                <li class="m-nav__item m-nav__separator"><i class="fa fa-angle-double-right"></i></li>
                <li class="m-nav__item">
                    <a href="javascript:return;" class="m-nav__link">
                        <span class="m-nav__link-text">Edit</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>

<div class="m-content" style="padding-left:20px; padding-right:20px">
    <div class="row">
        <div class="col-xs-12 col-md-12 box-content" style="padding-top: 15px;">
            <div class="box-body no-padding">
                @{
                    var route = "/Marketing/Utility/Edit";
                }
                <form id="form-mf" @(route != "" ? "action=" + route + "" : "") data-title-action="save" method="post" onsubmit="return handleSubmit(this, submitData)">
