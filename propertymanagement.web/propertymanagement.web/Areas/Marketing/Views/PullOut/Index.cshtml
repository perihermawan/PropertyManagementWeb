﻿@{
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<div class="m-subheader">
    <div class="d-flex align-items-center">
        <div class="mr-auto">
            <h2 class="m-subheader__title m-subheader__title--separator">Marketing Pull Out&nbsp;&nbsp;</h2>
            <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
                <li class="m-nav__item m-nav__item--home">
                    <a href="javascript:return;" class="m-nav__link m-nav__link--icon">
                        <i class="fa fa fa-calendar"></i>
                    </a>
                </li>
                <li class="m-nav__item m-nav__separator"><i class="fa fa-angle-double-right"></i></li>
                <li class="m-nav__item">
                    <a href="javascript:return;" class="m-nav__link">
                        <span class="m-nav__link-text">List Of Pull Out</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>

@*</br></br><div style="font-weight: bold;font-size: 110%;font-family: Sans-Serif;">Loading</div><div style="font-weight: bold;font-size: 110%;font-family: Sans-Serif;">Please wait...</div></br></br>
    <img src="../image/loading1.gif" /></br></br>*@
<div class="m-content" style="padding-left:20px; padding-right:20px">
    <div class="row">
        <div class="col-xs-12 box-content">
            <div class="portlet-body">
                <div class="box-body no-padding">
                    <form class="form-horizontal" role="form" id="form-pullout">
                        <div class="form-body">
                            <div id="datatable_fixed_column_wrapper" class="dataTables_wrapper form-inline no-footer">
                                <div class="dt-toolbar">
                                </div>
                                <table id="pullout_table_header" class="datatable table table-striped table-responsive table-bordered dataTable no-footer" role="grid" style="font-size:12px">
                                    <thead style="font-size:10px">
                                        <tr role="row">
                                            <th tabindex="0" width="5%">NO</th>
                                            <th tabindex="0" headers="hidden" aria-controls="pullout_table_header" id="rentId" aria-sort="ascending">Rent ID</th>
                                            <th tabindex="0" width="15%" aria-controls="pullout_table_header" id="ksmNumber" aria-sort="ascending">KSM Number</th>
                                            <th tabindex="0" width="10%" aria-controls="pullout_table_header" id="locationMap" aria-sort="ascending">Location</th>
                                            <th tabindex="0" width="10%" aria-controls="pullout_table_header" id="square" aria-sort="ascending">Rent Square</th>
                                            <th tabindex="0" width="15%" aria-controls="pullout_table_header" id="outletName" aria-sort="ascending">Outlet</th>
                                            <th tabindex="0" width="15%" aria-controls="pullout_table_header" id="tenantOwner" aria-sort="ascending">Tenant</th>
                                            <th tabindex="0" width="10%" aria-controls="pullout_table_header" id="startDate" aria-sort="ascending">Start Date</th>
                                            <th tabindex="0" width="10%" aria-controls="pullout_table_header" id="endDate" aria-sort="ascending">End Date</th>
                                            <th tabindex="0" width="15%" aria-controls="pullout_table_header" id="createUser" aria-sort="ascending">Create User</th>
                                            <th tabindex="0" width="15%" aria-controls="pullout_table_header" id="updateUser" aria-sort="ascending">Update User</th>
                                            <th tabindex="0" width="5%" aria-controls="pullout_table_header">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <div class="dt-toolbar-footer">
                                    <div class="col-sm-6 col-xs-12 hidden-xs"></div>
                                    <div class="col-xs-12 col-sm-6"></div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts{
    <script type="text/javascript" src="~/js/jsfile/PullOut/Index.js?v1.4"></script>

}
