@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using propertymanagement.web.Areas.Marketing.Models.ViewModel;
@model propertymanagement.web.Areas.Marketing.Models.DTO.ViewTenantMgtRentSales;
@{
    var action = ViewBag.Action;
    var code = ViewBag.Code;
    var tenantMgt = Model != null ? Model : null;

    var dataSCRent = new ViewTenantMgtRentModel();
    var dataSCSales = new ViewTenantMgtSalesModel();
    var dataPLRent = new ViewTenantMgtRentModel();
    var dataPLSales = new ViewTenantMgtSalesModel();
    var dataFLRent = new ViewTenantMgtRentModel();
    var dataFLSales = new ViewTenantMgtSalesModel();
    if (tenantMgt != null)
    {
        if (code == "Rent")
        {
            if (tenantMgt.tenantRent.Count > 0)
            {
                foreach (var item in tenantMgt.tenantRent)
                {
                    if (item.Code == "SC")
                    {
                        dataSCRent = item;
                    }
                    else if (item.Code == "PL")
                    {
                        dataPLRent = item;
                    }
                    else if (item.Code == "FL")
                    {
                        dataFLRent = item;
                    }
                }
            }
        }
        else if (code == "Sales")
        {
            if (tenantMgt.tenantSales.Count > 0)
            {
                foreach (var item in tenantMgt.tenantSales)
                {
                    if (item.Code == "SC")
                    {
                        dataSCSales = item;
                    }
                    else if (item.Code == "PL")
                    {
                        dataPLSales = item;
                    }
                    else if (item.Code == "FL")
                    {
                        dataFLSales = item;
                    }
                }
            }
        }
    }
}

<style type="text/css">
    .req:after {
        content: " *";
        color: red;
    }

    .col-no-padding {
        padding: 0px;
    }

    .line-separator-1 {
        width: 100%;
        margin: 5px 0px 15px;
        border: 1px solid #bababa3d;
    }

    #form-mf input, #form-mf select {
        max-width: unset;
    }

    @@media screen and (min-width: 992px) {
        .modal-lg {
            width: 988px;
        }
    }

    .btn {
        border-radius: 4px !important;
    }
</style>

<div class="m-subheader">
    <div class="d-flex align-items-center">
        <div class="mr-auto">
            <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
                <li class="m-nav__item">
                    <a asp-action="Index" asp-controller="Unit" class="m-nav__link">
                        <span class="m-nav__link-text">Maintenance Fee</span>
                    </a>
                </li>
                <li class="m-nav__item m-nav__separator"><i class="fa fa-angle-double-right"></i></li>
                <li class="m-nav__item">
                    <a href="javascript:return;" class="m-nav__link">
                        <span class="m-nav__link-text">Edit</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>

<div class="m-content" style="padding-left:20px; padding-right:20px">
    <div class="row">
        <div class="col-xs-12 col-md-12 box-content" style="padding-top: 15px;">
            <div class="box-body no-padding">
                @{
                    var route = "/Marketing/MF/Edit";
                }
                <form id="form-mf" @(route != "" ? "action=" + route + "" : "") data-title-action="save" method="post" onsubmit="return handleSubmit(this, submitData)">
                    <input type="hidden" name="sourceCode" value="@(code == "Rent" ? tenantMgt.tenantRent[0].SourceCode : code == "Sales" ? tenantMgt.tenantSales[0].SourceCode : "")" />
