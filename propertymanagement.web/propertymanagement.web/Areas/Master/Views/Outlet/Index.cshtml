@using propertymanagement.web.Areas.Master.Models;
@model propertymanagement.web.Areas.Master.Models.Outlet;

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var action = ViewBag.Action;
    var outlet = Model != null ? Model : null;
}

<div class="m-subheader">
    <div class="d-flex align-items-center">
        <div class="mr-auto">
            <h2 class="m-subheader__title m-subheader__title--separator">Master Outlet&nbsp;&nbsp;</h2>
            <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
                <li class="m-nav__item m-nav__item--home">
                    <a href="javascript:return;" class="m-nav__link m-nav__link--icon">
                        <i class="fa fa fa-calendar"></i>
                    </a>
                </li>
                <li class="m-nav__item m-nav__separator"><i class="fa fa-angle-double-right"></i></li>
                <li class="m-nav__item">
                    <a href="javascript:return;" class="m-nav__link">
                        <span class="m-nav__link-text">List Of Outlet</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>
@*</br></br><div style="font-weight: bold;font-size: 110%;font-family: Sans-Serif;">Loading</div><div style="font-weight: bold;font-size: 110%;font-family: Sans-Serif;">Please wait...</div></br></br>
    <img src="../image/loading1.gif" /></br></br>*@
<div class="m-content" style="padding-left:20px; padding-right:20px">
    <div class="row">
        <div class="col-xs-12 box-content">
            <div class="portlet-body">
                <div class="box-body no-padding">
                    <form class="form-horizontal" role="form" id="form-outlet">
                        <div class="form-body">
                            <div id="datatable_fixed_column_wrapper" class="dataTables_wrapper form-inline no-footer">
                                <div class="dt-toolbar">
                                </div>
                                <table id="outlet_table_header" class="datatable table table-striped table-responsive table-bordered dataTable no-footer" role="grid" style="font-size:12px">
                                    <thead style="font-size:10px">
                                        <tr role="row">
                                            <th tabindex="0" width="10%">NO</th>
                                            <th tabindex="0" aria-controls="outlet_table_header" headers="hidden" id="outletId" aria-sort="ascending" aria-label="ID">ID</th>
                                            @*<th tabindex="0" aria-controls="unit_table_header" width="20%" id="buildingName" aria-sort="ascending">Building</th>*@
                                            <th tabindex="0" aria-controls="outlet_table_header" id="outletName" aria-sort="ascending">Outlet Name</th>
                                            @*<th tabindex="0" aria-controls="employee_table_header" id="outletTypeName" aria-sort="ascending">Outlet Type</th>*@
                                            @*<th tabindex="0" aria-controls="unit_table_header" id="Sex" aria-sort="ascending">Sex</th>*@
                                            <th tabindex="0" aria-controls="outlet_table_header" id="lobName" aria-sort="ascending">Lob Name</th>
                                            @*<th tabindex="0" aria-controls="unit_table_header" id="Email" aria-sort="ascending">Email</th>*@
                                            <th tabindex="0" aria-controls="outlet_table_header">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <div class="dt-toolbar-footer">
                                    <div class="col-sm-6 col-xs-12 hidden-xs"></div>
                                    <div class="col-xs-12 col-sm-6"></div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="divmodal">
    @Html.Partial("ModalOutlet")
</div>

@section Scripts{
    <script type="text/javascript" src="~/js/jsfile/Outlet/Index.js?v1.0"></script>
    <script type="text/javascript">
        var action = "@action";
        
    </script>
}

